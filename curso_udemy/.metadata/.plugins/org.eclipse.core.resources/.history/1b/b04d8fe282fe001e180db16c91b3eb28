
public class ExAluno {

	public static void main(String[] args) {
		Aluno ester = new Aluno();
		
	     ester.nome = "Ester";
	     ester.matricula = "40028922";
	     
	    

	    Aluno vitor = new Aluno();
	    
	    vitor.nome = "vitor";
	    vitor.matricula ="123456";
	    
	    vitor.mediaDasNotas(9, 2, 4, 3);
	    vitor.situaçãoDoAluno(4.5);
	    
	    
	    
	}

}
